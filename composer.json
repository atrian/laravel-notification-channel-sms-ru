{
    "name": "fomvasss/laravel-notification-channel-sms-ru",
    "description": "This package makes it easy to send notifications using [sms.ru](https://sms.ru) (aka SMSRU) with Laravel 5.5+",
    "homepage": "https://github.com/fomvasss/laravel-notification-channel-sms-ru",
    "keywords": ["laravel", "notifications", "sms", "sms-ru", "СМС.RU"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fomin Vasil",
            "email": "fomvasss@gmail.com",
            "homepage": "https://github.com/fomvasss",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0",
        "zelenin/smsru": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SmsRu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SmsRu\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\SmsRu\\SmsRuServiceProvider"
            ]
        }
    }
}
